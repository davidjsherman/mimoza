title = {{BiGG: a Biochemical Genetic and Genomic knowledgebase of large scale metabolic reconstructions.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2874806\&tool=pmcentrez\&rendertype=abstract},
