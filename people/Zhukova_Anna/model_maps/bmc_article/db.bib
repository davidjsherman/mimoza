pages = {1254--1265},
shorttitle = {Proceedings of the IEEE},
title = {{CellDesigner 3.5: A Versatile Modeling Tool for Biochemical Networks}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=4567412},
volume = {96},
year = {2008}
}
